#include<iostream>
using namespace std;
int MAX(int a, int b, int c) {
    if (a > b) {
        if (a > c) {
            return a;
        }
        else
            return c;
    }
    else {
        if (b > c) {
            return b;
        }
        else return c;
       
    }
        
    
}   int MIN(int a, int b, int c) {
    if (a < b) {
        if (a < c) {
            return a;
        }
        else
            return c;
    }
    else {
        if (b < c) {
            return b;
        }
        else return c;
    }
}


int main() {
    int a, b, c;
    cin >> a >> b >> c;
    int giatrilonnhat;
    giatrilonnhat = MAX(a,b,c);
    cout << giatrilonnhat;
    
    

}
